{
  "name": "profile",
  "version": "1.0.0",
  "description": "Install Dependencies",
  "main": "",
  "scripts": {
    "preinstall": "go mod download; go install github.com/a-h/templ/cmd/templ@latest",
    "astro": "astro",
    "dev": "bunx --bun astro dev",
    "preview": "bunx serve public",
    "__build__": "",
    "build:all": "bun run generate; bun run build",
    "build": "bun run build:home; bun run build:blog",
    "build:home": "go run cmd/main.go",
    "build:blog": "bunx --bun astro build",
    "__generate__": "",
    "generate": "bun run generate:html; bun run generate:css",
    "generate:html": "templ generate",
    "generate:css": "bunx @tailwindcss/cli -i ./index.css -o public/styles.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ism0080@github.com/ism0080/profile.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ism0080/profile/issues"
  },
  "homepage": "https://github.com/ism0080/profile#readme",
  "dependencies": {
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.3.1",
    "@tailwindcss/cli": "^4.1.5",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.5",
    "astro": "^5.7.10",
    "mdast-util-to-string": "^4.0.0",
    "onedollarstats": "^0.0.11",
    "reading-time": "^1.5.0",
    "rehype-pretty-code": "^0.14.1",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/bun": "^1.2.11"
  }
}